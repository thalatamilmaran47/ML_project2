      "source": [
        "# -----------------------------\n",
        "# 1. Import Libraries\n",
        "# -----------------------------\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "from sklearn.cluster import KMeans\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.metrics import silhouette_score\n",
        "from sklearn.decomposition import PCA\n",
        "\n",
        "# -----------------------------\n",
        "# 2. Load Dataset\n",
        "# -----------------------------\n",
        "# Example dataset: CustomerID, Age, Annual Income, Spending Score\n",
        "# You can replace with your dataset (CSV, Excel, etc.)\n",
        "data = {\n",
        "    \"CustomerID\": range(1, 11),\n",
        "    \"Age\": [19, 35, 26, 27, 19, 27, 27, 32, 25, 40],\n",
        "    \"Annual Income (k$)\": [15, 35, 50, 40, 15, 70, 60, 80, 55, 75],\n",
        "    \"Spending Score (1-100)\": [39, 81, 6, 77, 40, 76, 50, 20, 47, 40]\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "print(\"âœ… Dataset Loaded\")\n",
        "print(df.head())\n",
        "\n",
        "# -----------------------------\n",
        "# 3. Preprocessing\n",
        "# -----------------------------\n",
        "X = df[[\"Annual Income (k$)\", \"Spending Score (1-100)\"]]  # features\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "# -----------------------------\n",
        "# 4. Find Optimal K (Elbow Method)\n",
        "# -----------------------------\n",
        "inertia = []\n",
        "K_range = range(1, 10)\n",
        "\n",
        "for k in K_range:\n",
        "    model = KMeans(n_clusters=k, random_state=42, n_init=10)\n",
        "    model.fit(X_scaled)\n",
        "    inertia.append(model.inertia_)\n",
        "\n",
        "plt.figure(figsize=(6,4))\n",
        "plt.plot(K_range, inertia, marker=\"o\")\n",
        "plt.title(\"Elbow Method for Optimal K\")\n",
        "plt.xlabel(\"Number of clusters (k)\")\n",
        "plt.ylabel(\"Inertia\")\n",
        "plt.show()\n",
        "\n",
        "# -----------------------------\n",
        "# 5. Apply K-Means\n",
        "# -----------------------------\n",
        "kmeans = KMeans(n_clusters=3, random_state=42, n_init=10)\n",
        "df[\"Cluster\"] = kmeans.fit_predict(X_scaled)\n",
        "\n",
        "print(\"\\nðŸ“Š Cluster Centroids (Scaled):\")\n",
        "print(kmeans.cluster_centers_)\n",
        "\n",
        "# -----------------------------\n",
        "# 6. Evaluation (Silhouette Score)\n",
        "# -----------------------------\n",
        "score = silhouette_score(X_scaled, df[\"Cluster\"])\n",
        "print(\"âœ… Silhouette Score:\", round(score, 3))\n",
        "\n",
        "# -----------------------------\n",
        "# 7. Visualization\n",
        "# -----------------------------\n",
        "plt.figure(figsize=(6,5))\n",
        "sns.scatterplot(\n",
        "    x=\"Annual Income (k$)\", y=\"Spending Score (1-100)\",\n",
        "    hue=\"Cluster\", palette=\"Set1\", data=df, s=100\n",
        ")\n",
        "plt.scatter(\n",
        "    kmeans.cluster_centers_[:,0]*X[\"Annual Income (k$)\"].std() + X[\"Annual Income (k$)\"].mean(),\n",
        "    kmeans.cluster_centers_[:,1]*X[\"Spending Score (1-100)\"].std() + X[\"Spending Score (1-100)\"].mean(),\n",
        "    s=200, c=\"black\", marker=\"X\", label=\"Centroids\"\n",
        ")\n",
        "plt.title(\"Customer Segmentation (K-Means Clusters)\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# -----------------------------\n",
        "# 8. Optional: PCA for Dimensionality Reduction\n",
        "# -----------------------------\n",
        "pca = PCA(n_components=2)\n",
        "X_pca = pca.fit_transform(X_scaled)\n",
        "\n",
        "plt.figure(figsize=(6,5))\n",
        "sns.scatterplot(x=X_pca[:,0], y=X_pca[:,1], hue=df[\"Cluster\"], palette=\"Set2\", s=100)\n",
        "plt.title(\"Clusters Visualized with PCA\")\n",
        "plt.show()"
      ]
    }
  ]
}